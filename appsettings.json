{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5055"
      },
      "Https": {
        "Url": "https://localhost:5051"
      }
    }
  },
  "ConnectionStrings": {
    "AutoRabbitMQ": "amqp://guest:guest@localhost:5672"
  },
  "profiles": {
    "GrpcService": {
      "applicationUrl": "https://localhost:5051;http://localhost:5055",
      "dotnetRunMessages": true
    }
  }
}
